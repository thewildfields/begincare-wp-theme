{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultPalette": false,
            "palette": [
                {
                    "name": "Teal",
                    "slug": "teal",
                    "color": "#7CAAB7"
                },
                {
                    "name": "Light Teal",
                    "slug": "light-teal",
                    "color": "#D8F6FF"
                },
                {
                    "name": "Light Sand",
                    "slug": "light-sand",
                    "color": "#F3F4ED"
                },
                {
                    "name": "Gray",
                    "slug": "gray",
                    "color": "#484E4F"
                },
                {
                    "name": "Black",
                    "slug": "Black",
                    "color": "#000000"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#FFFFFF"
                }
            ]
        },
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1800px"
        },
        "spacing": {
            "defaultSpacingSizes": false,
            "spacingSizes": [
                {
                    "name": "XS",
                    "slug": "xs",
                    "size": "10px"
                },
                {
                    "name": "S",
                    "slug": "s",
                    "size": "20px"
                },
                {
                    "name": "M",
                    "slug": "m",
                    "size": "40px"
                },
                {
                    "name": "L",
                    "slug": "l",
                    "size": "80px"
                },
                {
                    "name": "XL",
                    "slug": "xl",
                    "size": "120px"
                },
                {
                    "name": "XXL",
                    "slug": "xxl",
                    "size": "160px"
                }
            ]
        },
        "typography": {
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFamily": "Lato",
                    "name": "Lato",
                    "slug": "lato",
                    "fontFace": [
                        {
                            "fontFamily": "Lato",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/Lato-Light.woff",
                                "file:./assets/fonts/Lato-Light.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Lato",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/Lato-Regular.woff",
                                "file:./assets/fonts/Lato-Regular.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Lato",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "fontStretch": "normal",
                            "src": [
                                "file:./assets/fonts/Lato-Bold.woff",
                                "file:./assets/fonts/Lato-Bold.woff2"
                            ]
                        }
                    ]
                }
            ],
            "defaultFontSizes": false,
            "fontSizes": [
                {
                    "name": "S",
                    "slug": "s",
                    "size": "0.875rem"
                },
                {
                    "name": "M",
                    "slug": "m",
                    "size": "1rem"
                },
                {
                    "name": "L",
                    "slug": "l",
                    "size": "1.5rem"
                },
                {
                    "name": "XL",
                    "slug": "xl",
                    "size": "3.5rem",
                    "fluid": {
                        "min": "1.5rem",
                        "max": "3.5rem"
                    }
                },
                {
                    "name": "XXL",
                    "slug": "xxl",
                    "size": "6rem",
                    "fluid": {
                        "min": "2.5rem",
                        "max": "6rem"
                    }
                }
            ]
        }
    },
    "styles": {
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--lato)",
            "fontWeight": "400",
            "fontSize": "var(--wp--preset--font-size-l)",
            "lineHeight": "1.5"
        },
        "elements": {
            "link": {
                "typography": {
                    "textDecoration": "none"
                },
                "color": {
                    "text": "inherit"
                }
            },
            "heading": {
                "typography": {
                    "fontWeight": "700"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xxl)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xl)"
                },
                "spacing": {
                    "margin": {
                        "bottom": "var(--wp--preset--spacing--l)"
                    }
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                },
                "spacing": {
                    "margin": {
                        "bottom": "var(--wp--preset--spacing--l)"
                    }
                }
            }
        },
        "blocks": {
            "core/group": {
                "spacing": {
                    "blockGap": "0px"
                }
            },
            "core/paragraph":{
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--s)",
                        "bottom": "var(--wp--preset--spacing--m)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                }
            },
            "core/list": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                }
            },
            "core/list-item": {
                "spacing": {
                    "margin": {
                        "bottom": "var(--wp--preset--spacing--s)"
                    }
                }
            }
        },
        "css": ".entry-content{margin-top: 0!important,margin-bottom: 0!important,margin-left: auto,margin-right: auto} @media screen and (max-width: 1840px){.wp-site-blocks > .wp-block-template-part > *, .wp-site-blocks > .wp-block-post-content > *{padding-left: 20px;padding-right: 20px}}"
    }
}